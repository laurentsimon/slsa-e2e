{
    "version": 1,
    "tracks": {
        "build": {
            "builders": [
                {
                    "id": "https://github.com/slsa-framework/slsa-github-generator/.github/workflows/generator_container_slsa3.yml",
                    "level": 3
                },
                {
                    "id": "https://cloudbuild.googleapis.com/GoogleHostedWorker",
                    "level": 3
                }
            ]
        },
        "source": {
            "attestors": [
                {
                    "id": "https://github.com/source-attestor",
                    "authoritativeFor": [
                        "git+https://github.com/*"
                    ]
                },
                {
                    "id": "https://cloudbuild.googleapis.com/GoogleSourceAttestor",
                    "authoritativeFor": [
                        "git+https://git.googlecloud.com/*"
                    ]
                }
            ]
        }
    },
    "images": [
        {
            "uri": "docker://google/*"
        },
        {
            "uri": "docker://googlecloud/*"
        }
    ],
    "sources": [
        {
            "uri": "git+https://github.com/google/*"
        },
        {
            "uri": "git+https://github.com/googlecloudplatform/*"
        }
    ],
    "enforcement": {
        "onViolation": "allow",
        "logOn": [
            "deny", "allow"
        ],
        "overwrite": {
            "default": "deny",
            "exceptions": [
                {
                    "sources": [
                        {
                            "uri": "git+https://github.com/googlenot/*"
                        },
                        {
                            "uri": "git+https://github.com/google/special2"
                        }
                    ],
                    "reason": "special repos",
                    "overwrite": "allow",
                    "onViolation": "deny"
                },
                {
                    "sources": [
                        {
                            "uri": "git+https://github.com/another/repo1"
                        },
                        {
                            "uri": "git+https://github.com/another/repo2"
                        }
                    ],
                    "reason": "other repos",
                    "overwrite": "allow"
                }
            ]
        }
    }
}